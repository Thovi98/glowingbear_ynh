{
  "name": "Glowing Bear",
  "id": "glowingbear",
  "packaging_format": 1,
  "description": {
    "en": "Web client for WeeChat",
    "fr": "Client Web pour WeeChat"
  },
  "version": "0.9.0~ynh1",
  "url": "https://www.glowing-bear.org",
  "license": "AGPL-3.0",
  "maintainer": {
    "name": "jodeko",
    "email": "jodeko@riseup.net"
  },
  "requirements": {
        "yunohost": ">= 11.0.9"
  },
  "multi_instance": true,
  "services": [
    "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
        {
                "name": "path",
                "type": "path",
                "example": "/glowing-bear",
                "default": "/glowing-bear"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": false
            }
        ]
    }
}
